@model UpdateFeatureDto

@{
    ViewData["Title"] = "CreateCategory";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}


<section class="content container-fluid">
    <div class="row">

        <div class="col-md-12">
            <div class="chart-box">
                <div class="bs-example" data-example-id="bordered-table">

                    <form method="post">
                        <input type="hidden" asp-for="Id" class="form-control" />
                        <label for="name">Öne Çıkan Alan Adı</label>
                        <input type="text" asp-for="Title" class="form-control" />
                        <br />
                        <label for="name">Öne Çıkan Alan İkonu</label>
                        <input type="text" asp-for="Icon" class="form-control" />
                        <br />
                        <button type="submit" class="btn btn-success">Öne Çıkan Alan Güncelle</button>
                    </form>
                </div>
            </div>
        </div>

    </div>
</section>